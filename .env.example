# Backend API
REACT_APP_BACKEND_URL=https://your-backend-url.com

# Supabase Configuration (REQUIRED)
# Get these from your Supabase project settings
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here
# Optional: Service key for server-side operations
REACT_APP_SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Twilio Configuration (Optional - for calling features)
REACT_APP_TWILIO_FUNCTION_URL=https://your-twilio-function-url
REACT_APP_TWILIO_API_KEY=your_twilio_api_key_here
REACT_APP_TWILIO_PHONE_NUMBER=+1234567890
REACT_APP_TWILIO_ACCOUNT_SID=your_twilio_account_sid
REACT_APP_TWILIO_AUTH_TOKEN=your_twilio_auth_token

# Encryption key (generate a secure random string)
REACT_APP_ENCRYPTION_KEY=your_encryption_key_here

# Stripe Configuration (Optional - for payment features)
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PRICE_ID_PROFESSIONAL_MONTHLY=price_your_stripe_price_id
STRIPE_PRICE_ID_PROFESSIONAL_ANNUAL=price_your_stripe_price_id
STRIPE_PRICE_ID_INSIGHTS_MONTHLY=price_your_stripe_price_id
STRIPE_PRICE_ID_INSIGHTS_ANNUAL=price_your_stripe_price_id
STRIPE_PRICE_ID_ENTERPRISE=price_your_stripe_price_id
STRIPE_SUCCESS_URL=http://localhost:3000/subscribe/success
STRIPE_CANCEL_URL=http://localhost:3000/subscribe/cancel

# Additional Services (Optional)
REACT_APP_SPHERE1A_URL=https://your-sphere1a-url.com
REACT_APP_SPHERE1A_API_KEY=your_sphere1a_api_key
REACT_APP_MARKET_DATA_SUPABASE_URL=https://your-market-data-project.supabase.co
REACT_APP_MARKET_DATA_SUPABASE_ANON_KEY=your_market_data_supabase_anon_key

# OpenRouter Configuration (Optional - for AI features)
REACT_APP_OPENROUTER_API_KEY=sk-or-v1-your-openrouter-key
REACT_APP_OPENROUTER_URL=https://openrouter.ai/api/v1/chat/completions

# Node Environment
NODE_ENV=development

# Admin Configuration
# Comma-separated list of admin email addresses
VITE_ADMIN_EMAILS=admin@example.com,admin2@example.com

# Sentry Configuration (Optional - for error monitoring)
# Get your DSN from https://sentry.io/
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_NODE_ENV=production