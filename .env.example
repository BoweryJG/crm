# Development Environment Configuration
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration (Required)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google OAuth (Unified - Required for authentication)
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here
REACT_APP_GOOGLE_REDIRECT_URI=http://localhost:7003/auth/google/callback

# Backend Configuration (Required)
REACT_APP_BACKEND_URL=https://osbackend-zl1h.onrender.com

# Stripe Configuration (Required for subscriptions)
STRIPE_PUBLISHABLE_KEY=pk_test_or_live_key_here
STRIPE_SECRET_KEY=sk_test_or_live_key_here

# Twilio Configuration (Required for RepÂ² and above)
REACT_APP_TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
REACT_APP_TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
REACT_APP_TWILIO_API_KEY=your_twilio_api_key_here
REACT_APP_TWILIO_API_SECRET=your_twilio_api_secret_here
REACT_APP_TWILIO_PHONE_NUMBER=+1234567890
TWILIO_TWIML_APP_SID=your_twiml_app_sid_here

# AI Services (Optional)
REACT_APP_OPENROUTER_API_KEY=your_openrouter_api_key_here
REACT_APP_SPHERE1A_API_KEY=your_sphere1a_api_key_here

# Google Services (Optional)
REACT_APP_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Third-party APIs (Optional)
REACT_APP_YELP_API_KEY=your_yelp_api_key_here

# Development Settings
PORT=7003
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG_MODE=true

# Encryption (Required for secure features)
REACT_APP_ENCRYPTION_KEY=your-32-character-encryption-key

# Feature Flags
REACT_APP_ENABLE_GMAIL=true
REACT_APP_ENABLE_TWILIO=true
REACT_APP_ENABLE_AI=true
