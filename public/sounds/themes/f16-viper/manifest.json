{
  "id": "f16-viper",
  "name": "F-16 Viper Fighter Jet",
  "description": "Tactical fighter jet cockpit with authentic military aviation sounds",
  "version": "1.0.0",
  "author": "Omnisensory Experience Engine",
  "sounds": {
    "theme-switch": {
      "id": "theme-switch",
      "url": "/sounds/themes/f16-viper/master-arm-switch.opus",
      "category": "theme-switch",
      "volume": 0.9,
      "description": "Master arm switch activation"
    },
    "ui-click-primary": {
      "id": "ui-click-primary", 
      "url": "/sounds/themes/f16-viper/pickle-button.opus",
      "category": "ui-primary",
      "volume": 0.8,
      "description": "Weapon release pickle button",
      "variations": [
        "/sounds/themes/f16-viper/weapons-select.opus",
        "/sounds/themes/f16-viper/trigger-first-detent.opus"
      ]
    },
    "ui-click-secondary": {
      "id": "ui-click-secondary",
      "url": "/sounds/themes/f16-viper/radio-transmit.opus", 
      "category": "ui-secondary",
      "volume": 0.7,
      "description": "UHF radio transmit button"
    },
    "ui-hover": {
      "id": "ui-hover",
      "url": "/sounds/themes/f16-viper/mfd-button.opus",
      "category": "ui-secondary", 
      "volume": 0.4,
      "description": "Multi-function display button feedback"
    },
    "ui-toggle": {
      "id": "ui-toggle",
      "url": "/sounds/themes/f16-viper/gear-handle.opus",
      "category": "ui-primary",
      "volume": 0.9,
      "description": "Landing gear handle actuation"
    },
    "navigation-forward": {
      "id": "navigation-forward",
      "url": "/sounds/themes/f16-viper/hud-select.opus",
      "category": "navigation",
      "volume": 0.7,
      "description": "HUD mode selection"
    },
    "navigation-back": {
      "id": "navigation-back", 
      "url": "/sounds/themes/f16-viper/hud-deselect.opus",
      "category": "navigation",
      "volume": 0.7,
      "description": "HUD mode deselection"
    },
    "gauge-tick": {
      "id": "gauge-tick",
      "url": "/sounds/themes/f16-viper/aoa-indexer.opus",
      "category": "gauge",
      "volume": 0.3,
      "description": "Angle of attack indexer tick"
    },
    "gauge-zone-change": {
      "id": "gauge-zone-change",
      "url": "/sounds/themes/f16-viper/rwr-new-threat.opus", 
      "category": "gauge",
      "volume": 0.8,
      "description": "RWR new threat indication"
    },
    "gauge-peak": {
      "id": "gauge-peak",
      "url": "/sounds/themes/f16-viper/bingo-fuel.opus",
      "category": "gauge", 
      "volume": 0.9,
      "description": "Bingo fuel warning"
    },
    "notification-success": {
      "id": "notification-success",
      "url": "/sounds/themes/f16-viper/fox-two.opus",
      "category": "notification",
      "volume": 0.9,
      "description": "Fox Two missile launch confirmation"
    },
    "notification-error": {
      "id": "notification-error", 
      "url": "/sounds/themes/f16-viper/missile-warning.opus",
      "category": "error",
      "volume": 1.0,
      "description": "Missile launch warning"
    },
    "notification-info": {
      "id": "notification-info",
      "url": "/sounds/themes/f16-viper/datalink-update.opus", 
      "category": "notification",
      "volume": 0.7,
      "description": "Datalink update notification"
    },
    "rwr-lock": {
      "id": "rwr-lock",
      "url": "/sounds/themes/f16-viper/rwr-lock-tone.opus",
      "category": "error",
      "volume": 1.0,
      "description": "Radar lock warning tone"
    },
    "startup-1": {
      "id": "startup-1",
      "url": "/sounds/themes/f16-viper/jet-fuel-start.opus",
      "category": "startup",
      "volume": 0.7
    },
    "startup-2": {
      "id": "startup-2", 
      "url": "/sounds/themes/f16-viper/engine-idle.opus",
      "category": "startup",
      "volume": 0.6
    },
    "startup-3": {
      "id": "startup-3",
      "url": "/sounds/themes/f16-viper/avionics-online.opus", 
      "category": "startup",
      "volume": 0.5
    },
    "startup-4": {
      "id": "startup-4",
      "url": "/sounds/themes/f16-viper/systems-ready.opus",
      "category": "startup", 
      "volume": 0.8
    }
  },
  "startupSequence": ["startup-1", "startup-2", "startup-3", "startup-4"],
  "ambient": {
    "id": "ambient",
    "url": "/sounds/themes/f16-viper/jet-cockpit-idle.opus",
    "category": "ambient",
    "volume": 0.3,
    "loop": true,
    "description": "F-16 cockpit idle ambience"
  }
}